{
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "workbench.iconTheme": "vscode-icons",
    "editor.formatOnSave": true,
    "window.zoomLevel": 1,
    // remote extensions
    "remote.SSH.defaultExtensions": [
        "golang.go-0.19.1",
        "ms-toolsai.jupyter-2020.11.399280825",
        "eamodio.gitlens-11.0.6",
        "ms-toolsai.jupyter-2020.12.411183155",
        "esbenp.prettier-vscode-5.8.0",
        "ms-azuretools.vscode-docker-1.8.1",
        "yzhang.markdown-all-in-one-3.4.0",
        "ms-python.python-2020.11.371526539"
    ],
    // python virtual env
    "python.pythonPath": "<python_path>",
    // python formatting
    "python.formatting.provider": "black",
    "[python]": {
        "editor.formatOnSave": true
    },
    "workbench.colorTheme": "Dracula",
    "git.autofetch": true,
    // dbt specific
    "files.associations": {
        "*.sql": "jinja-sql"
    //    "**/<dbt-project-dir>/**/*.yaml": "jinja-yaml",
    //    "**/<dbt-project-dir>/**/*.yml": "jinja-yaml",
    //    "**/<dbt-project-dir>/**/*.sql": "jinja-sql",
    //    "**/<dbt-project-dir>/**/docs/**/*.md": "jinja-md"
    }
}